@model WebApp.ViewModels.TicketCreateEditViewModel

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<div class="form-group">
    <label asp-for="Ticket.UserId" class="control-label"></label>
    <select asp-for="Ticket.UserId" class="form-control" asp-items="Model.Users"></select>
    <span asp-validation-for="Ticket.UserId" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Ticket.RaffleId" class="control-label"></label>
    <select asp-for="Ticket.RaffleId" class="form-control" asp-items="Model.Raffles"></select>
    <span asp-validation-for="Ticket.RaffleId" class="text-danger"></span>
</div>